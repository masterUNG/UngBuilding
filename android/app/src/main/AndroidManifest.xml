        android:label="ungbuilding"
        android:icon="@mipmap/ic_launcher">

        <meta-data android:name="com.google.android.geo.API_KEY"
               android:value="AIzaSyADX4JuITJhhio_4fx1MvWY64WkDQCfjnc"/>

        <activity
